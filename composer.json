{
    "name": "emnabs/yii2-plupload",
    "description": "Yii2 Plupload Widget",
    "keywords": ["yii2", "plupload", "extension"],
    "homepage": "https://github.com/emnabs/yii2-plupload",
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "emhome",
            "email": "emhome@163.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.14",
        "bower-asset/plupload": "2.3.6"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "emhome\\plupload\\": "src"
        }
    }
}